  "scripts": {
    "clean:npm:cache": "npm cache clean --force",
    "clean:react:native:cache": "react-native start --reset-cache",
    "eas:login": "eas login",
    "eas:update": "eas update",
    "eslint": "npx eslint --ext .ts,.tsx .",
    "eslint:fix": "npx eslint --fix --ext .ts,.tsx .",
    "expo:android": "expo start --clear --android",
    "expo:check": "npx expo install --check",
    "expo:doctor": "npx expo-doctor",
    "expo:eject": "expo eject",
    "expo:install": "expo install",
    "expo:ios": "expo start --clear --ios",
    "expo:publish": "expo publish",
    "expo:start": "expo start --clear",
    "expo:update": "npx expo install --fix",
    "expo:web": "expo start --clear --web",
    "npm:audit": "npm audit fix",
    "npm:install": "npm i",
    "postclean:npm:cache": "npm run npm:install",
    "postupdate:dependency": "npm run expo:update",
    "preclean:npm:cache": "rm -f package-lock.json && rm -rf node_modules",
    "preupdate:dependency": "npx npm-check-updates -u",
    "run:android:natively": "expo run:android",
    "reset:project": "node ./scripts/reset-project.js",
    "run:ios:device": "expo run:ios --device",
    "run:ios:natively": "expo run:ios",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "update:dependency": "npm run npm:install"
